{
  "name": "huphtur.nl",
  "description": "Personal blog built with Eleventy and Tailwind CSS.",
  "repository": "https://github.com/huphtur/huphtur.nl",
  "license": "MIT",
  "scripts": {
    "serve": "rm -rf _site && npx @11ty/eleventy --serve & npx tailwindcss -i ./src/tailwind.css -o ./_site/styles.css --watch",
    "build": "rm -rf _site && ELEVENTY_ENV=production npx @11ty/eleventy && npx tailwindcss -i ./src/tailwind.css -o ./_site/styles.css --minify"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@tailwindcss/typography": "^0.5.7",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "eleventy-plugin-embed-everything": "^1.14.1",
    "eleventy-plugin-img2picture": "^3.0.0"
  }
}
