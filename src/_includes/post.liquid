---
layout: default
---
<h1><a href="{{ post.url | url }}">{{ title }}</a></h1>
{% if date %}<time class="text-thc-pop dark:text-thc-pop-dark font-heading text-lg tracking-widest uppercase" datetime="{{ date | date: "%Y-%m-%d" }}">{{ date | date: "%B %d, %Y" }}</time>{% endif %}
{{ content | safe }}
